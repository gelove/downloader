{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "updater",
  "description": "Capability for the updater window",
  "windows": ["updater"],
  "platforms": ["macOS", "linux", "windows"],
  "permissions": [
    "core:event:default",
    "core:webview:default",
    "core:webview:allow-webview-close",
    "core:window:default",
    "core:window:allow-close",
    "dialog:default",
    "process:default",
    "updater:default"
  ]
}
